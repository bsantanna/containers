{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "docker-images-template",
    "annotations": {
      "description": "Personal collection of Docker Images: https://hub.docker.com/u/bsantanna/",
      "tags": "docker,image"
    }
  },
  "objects": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "docker-manifest-publisher"
      },
      "spec": {
        "tags": [
          {
            "from": {
              "kind": "DockerImage",
              "name": "docker.io/bsantanna/docker-manifest-publisher:latest"
            },
            "name": "latest"
          }
        ]
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "base-alpine-build",
        "labels": {
          "name": "base-alpine-build"
        },
        "annotations": {
          "template.alpha.openshift.io/wait-for-ready": "true"
        }
      },
      "spec": {
        "triggers": [
          {
            "type": "GitHub",
            "github": {
              "secret": "secret101"
            }
          },
          {
            "type": "Generic",
            "generic": {
              "secret": "secret101",
              "allowEnv": true
            }
          },
          {
            "type": "ImageChange",
            "imageChange": {}
          },
          {
            "type": "ConfigChange"
          }
        ],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/bsantanna/docker-images.git"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "dockerfilePath": "images/base/alpine/arch/x86_64/Dockerfile"
          }
        },
        "output": {
          "to": {
            "kind": "DockerImage",
            "name": "docker.io/bsantanna/alpine:x86_64"
          },
          "pushSecret": {
            "name": "dockerhub"
          }
        },
        "resources": {}
      },
      "status": {
        "lastVersion": 0
      }
    }
  ],
  "parameters": [],
  "labels": {
    "template": "docker-images-template"
  }
}